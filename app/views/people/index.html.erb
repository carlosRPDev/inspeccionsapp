<h1>Personas</h1>

<%= link_to "Agregar asesor(a)", new_person_path %>


<table>
  <tr>
    <th>Id</th>
    <th>Cedula</th>
    <th>Nombre</th>
    <th>Apellido</th>
  </tr>
  <% @person.each do |person| %>
    <tr>
      <td><%= person.id %></td>
      <td><%= person.cedula %></td>
      <td><%= person.name %></td>
      <td><%= person.last_name %></td>
    </tr>
  <% end %>
</table>
